@model IEnumerable<UCP1_PAW_077.Models.JenisProduk>

@{
    ViewData["Title"] = "Index";
}

<h1>Jenis Produk</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Jenis Produk")
            </th>
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nama)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.KodeJenisProduk"><i class="fas fa-edit"></i></a> |
                <a asp-action="Details" asp-route-id="@item.KodeJenisProduk"><i class="fas fa-info-circle"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.KodeJenisProduk"><i class="fas fa-trash-alt"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
